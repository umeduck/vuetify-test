{
  "plugins": ["vue"],
  "parser": "vue-eslint-parser",
  "parserOptions": {
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "es2022": true
  },
  "globals": {},
  "rules": {
    "semi": "warn",
    "no-irregular-whitespace": "warn",
    "no-multiple-empty-lines": "warn",
    "no-trailing-spaces": "warn",
    "vue/html-indent": "warn"
  }
}